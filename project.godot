[application]

config/name="MirageVN"
config/description="A VN engine."
run/main_scene="res://scenes/cutscene/Splash.tscn"
config/project_settings_override="user://project_settings.godot"
run/delta_sync_after_draw=true
boot_splash/image="res://art/ui/splashA.png"
config/icon="res://icon.png"

[autoload]

EngineSettings="*res://singletons/EngineSettings.gd"
Manager="*res://singletons/Manager.tscn"
EmitterFactory="*res://singletons/EmitterFactory.tscn"
Helpers="*res://singletons/Helpers.gd"
UserSettings="*res://singletons/UserSettings.gd"

[debug]

settings/fps/force_fps=144
settings/fps/force_fps.release=144

[display]

window/size/width=1280
window/size/height=720
window/dpi/allow_hidpi=true
window/energy_saving/keep_screen_on=false
window/stretch/mode="2d"
window/stretch/aspect="keep"
window/size/fullscreen.release=false
window/size/borderless.release=false
window/vsync/use_vsync.release=true
window/vsync/vsync_via_compositor.release=false
window/dpi/allow_hidpi.release=true

[gui]

theme/custom="res://default_theme.tres"

